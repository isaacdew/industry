{
    "name": "isaacdew/industry",
    "description": "Industry is a composer package that allows you to integrate your Laravel Eloquent Factories with AI to seed your database with realistic string data. This can be useful for product demos and manual QA.",
    "keywords": ["dev"],
    "license": "MIT",
    "archive": {
        "exclude": [
            "/tests",
            "/workbench",
            "/phpunit.xml",
            "testbench.yaml"
        ]
    },
    "autoload": {
        "psr-4": {
            "Isaacdew\\Industry\\Tests\\": "tests/",
            "Isaacdew\\Industry\\": "src/"
        }
    },
    "require": {
        "laravel/pint": "^1.25",
        "prism-php/prism": "^0.90.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "phpunit/phpunit": "^12.3",
        "phpunit/php-code-coverage": "^12.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Isaacdew\\Industry\\IndustryServiceProvider"
            ]
        }
    }
}
